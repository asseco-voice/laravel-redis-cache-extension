{
    "name": "asseco-voice/laravel-redis-cache-extension",
    "description": "Laravel Redis cache extension",
    "keywords": [
        "php",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4 || ^8.0",
        "illuminate/cache": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "mockery/mockery": "^1.3.1",
        "fakerphp/faker": "^1.9.1",
        "orchestra/testbench": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Asseco\\RedisCacheExtension\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Asseco\\RedisCacheExtension\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Asseco\\RedisCacheExtension\\RedisCacheExtensionServiceProvider"
            ]
        }
    }
}
